{
  "hosting": {
    "public": "out",
    "rewrites": [
      {
        "source": "/news",
        "destination": "/news.html"
      },
      {
        "source": "**",
        "destination": "/index.html"
      }
    ],
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ]
  }
}